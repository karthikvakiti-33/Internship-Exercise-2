    "def string_reverse(s):\n",
    "    reversed_string=\"\"\n",
    "    for char in s:\n",
    "        reversed_string=char+reversed_string\n",
    "    return reversed_string\n",
    "string=input(\"Enter a string:\")\n",
    "print(\"Reversed_string:\",string_reverse(string))\n"
   ]
